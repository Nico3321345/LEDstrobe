#include<FastLED.h>
#define NUM_LED 135
#define DATA_PIN 11
CRGB LEDS[NUM_LED];
Void setup(){
  FastLED.addLED<WS28128.DATA_PIN>(LEDs, NUM_LEDS);
}
Void loop(){
  for(int i=0;i<NUM_LEDS;i++){
    leds[i]=CRGB::Pink;
    FastLED.show();
    delay(300);
    leds[i]=CRGB::White;
    leds[(NUM_LEDS-i)]=CRGB::White
